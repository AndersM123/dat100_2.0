package basis_oppgaver;

public class B1 {

	public static void main(String[] args) {

		System.out.println("x i grader      x i radianer         sin(x)           cos(x)");
		System.out.println("-------------------------------------------------------------------");

		int x = 0;
		
		while(x <=180) {
			
		}
		
		
		for (int x = 0; x <= 180; x += 15) {
			double vinkel_i_radianer = ((Math.PI * x) / 180) * 100 / 100;

			double sin = Math.sin(x);
			double cos = Math.cos(x);

			System.out.println(x + "                  " + (double) Math.round(vinkel_i_radianer * 1000) / 1000
					+ "              " + (double) Math.round(cos * 1000) / 1000 + "              "
					+ (double) Math.round(sin * 1000) / 1000);
		}
		System.out.println("-------------------------------------------------------------------");
	}

}
